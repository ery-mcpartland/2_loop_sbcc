       LimitsPositioning->False], 
      RowBox[{
       StyleBox[
        SubsuperscriptBox["\[Integral]", "0", "1"],
        ScriptLevel->0], 
       RowBox[{
        RowBox[{"\[DifferentialD]", "z"}], 
        FractionBox[
         RowBox[{
          RowBox[{"ln", "(", 
           RowBox[{
            RowBox[{"s", " ", 
             SuperscriptBox["z", "2"]}], "+", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"-", "s"}], "+", 
               SubsuperscriptBox["m", "1", "2"], "-", 
               SubsuperscriptBox["m", "0", "2"]}], ")"}], "z"}], "+", 
            SubsuperscriptBox["m", "0", "2"], "-", 
            RowBox[{"i", " ", "\[CurlyEpsilon]"}]}], ")"}], "-", 
          RowBox[{"ln", "(", 
           RowBox[{
            SubscriptBox["m", "0"], 
            SubscriptBox["m", "2"]}], ")"}]}], 
         RowBox[{
          RowBox[{"s", " ", 
           SuperscriptBox["z", "2"]}], "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "s"}], "+", 
             SubsuperscriptBox["m", "1", "2"], "-", 
             SubsuperscriptBox["m", "0", "2"]}], ")"}], "z"}], "+", 
          SubsuperscriptBox["m", "0", "2"], "-", 
          RowBox[{"i", " ", "\[CurlyEpsilon]"}]}]]}]}]}]}]}], 
   TraditionalForm]], "InlineMath"],
 "."
}], "Notes",
